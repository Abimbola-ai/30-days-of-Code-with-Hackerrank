    "class Calculator:\n",
    "    def power(self,n,p):\n",
    "        self.n = n\n",
    "        self.p = p\n",
    "    \n",
    "        if n < 0 or p < 0:\n",
    "            raise Exception(\"n and p should be non-negative\")\n",
    "        else:\n",
    "             return(n**p)"
   ]
